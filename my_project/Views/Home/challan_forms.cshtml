@{
    ViewBag.Title = "challan_forms";
}








<!DOCTYPE html>
<html lang="en">
<head>






<META Http-Equiv="Cache-Control" Content="no-cache"/>
<META Http-Equiv="Pragma" Content="no-cache"/>
<META Http-Equiv="Expires" Content="0"/>



    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin-Portal</title>

    <link rel="stylesheet" type="text/css" href="/changing/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/changing/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="/changing/css/local.css" />

    <script type="text/javascript" src="/changing/js/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/changing/bootstrap/js/bootstrap.min.js"></script>

    <!-- you need to include the shieldui css and js assets in order for the charts to work -->
    <link rel="stylesheet" type="text/css" href="http://www.shieldui.com/shared/components/latest/css/light-bootstrap/all.min.css" />
    <script type="text/javascript" src="http://www.shieldui.com/shared/components/latest/js/shieldui-all.min.js"></script>
    <script type="text/javascript" src="http://www.prepbootstrap.com/Content/js/gridData.js"></script>

    


    <style>

    div.hidden {
    background-color: #00FF00;
    width: 100px;
    height: 100px;
    overflow: hidden;


}

    </style>


    <script type="text/javascript">
       

        $(document).ready(function()
        {
            $('#degree').change(function () {
                $.ajax({
                    url: '@Url.Action("get_degreefee")',
                    type: 'POST',
                    data:{ degree:$('#degree').val(),
                       session: $('#session').val()
                    },
                    success: function (data) {

                        if (data != false) {
                            
                            $('#install').val(data.install);
                            $('#month').val(data.monthly);


                        }
                        else {

                            alert("Some thing went wrong");
                        }
                    }

                });
            });
           




        });



        


    </script>


</head>



<body>
    <div id="wrapper">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/Home/adminpage">Admin Panel</a>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active"><a href="~/Home/adminpage"><i class="fa fa-bullseye"></i> Dashboard</a></li>
                    <li><a href="~/Home/fa_challan"><i class="fa fa-tasks"></i>FA</a></li>
                    <li><a href="~/Home/ba_challan"><i class="fa fa-tasks"></i>BA</a></li>
                    <li><a href="~/Home/ics_challan"><i class="fa fa-tasks"></i> ICS</a></li>
                   
                      <li><a href="~/Home/icom_challan"><i class="fa fa-tasks"></i> ICOM</a></li>
                     <li><a href="~/Home/bcom_challan"><i class="fa fa-tasks"></i> BCOM</a></li>
                     <li><a href="~/Home/fsc_engg_challan"><i class="fa fa-tasks"></i> FSC Pre Engg</a></li>
                    <li><a href="~/Home/fsc_med_challan"><i class="fa fa-tasks"></i> FSC Pre Medical</a></li>
                       
                    


                    
                </ul>
                <ul class="nav navbar-nav navbar-right navbar-user">
                    
                     <li class="dropdown user-dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Admin<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="adminpage"><i class="fa fa-user"></i> Profile</a></li>
                          
                            <li class="divider"></li>
                            <li><a href="/Home/logout"><i class="fa fa-power-off"></i> Log Out </a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
        
        <form action="generate_challan_form" method="post">


            <div>
            <label for="degree" class="col-md-2">
                Name:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="name" name="name"  >
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
            <br /> <br /> <br />



            <div>

        <label for="firstname" class="col-md-2">
                Select Degree:
            </label>
            <div class="col-md-9">

         @Html.DropDownList("session", (IEnumerable<SelectListItem>)ViewBag.session, new { @class = "form-control" })
            </div>
            <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
            <br /> <br />

             <div>

        <label for="firstname" class="col-md-2">
                Select Degree:
            </label>
            <div class="col-md-9">

         @Html.DropDownList("degree", (IEnumerable<SelectListItem>)ViewBag.degree, new { @class = "form-control" })
            </div>
            <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>  
            <br /> <br /> 

             <div>
            <label for="degree" class="col-md-2">
                Installment Fee:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="install" name="install"  >
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
            <br /> <br />

            <div>
            <label for="degree" class="col-md-2">
                Monthly Fee:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="month" name="month" readonly >
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
            <br /> <br />
             <div>
            <label for="password" class="col-md-2">
               Fee type:
            </label>
            <div class="col-md-9">
                <select name="fee_type" class="form-control">
                    <option value="Monthly Fee">Monthly Fee</option>
                    <option value="install">Installment</option>
                    </select>
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>

            <br /> <br />



        <div class="col-md-10">
                <button type="submit" id="submit" name="submit" class="btn btn-info" style="float:right">
                    Get Challan Form
                </button> 
        </div> 

     

		</div>
		
    </form>
      
</body>
</html>





