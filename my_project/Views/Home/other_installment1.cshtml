    <style>

    div.hidden {
    background-color: #00FF00;
    width: 100px;
    height: 100px;
    overflow: hidden;


}

    </style>

    <script type="text/javascript">
        function myfunction1() {
            var s = document.getElementById("fee_type");
            var s1 = s.value;
            if (s1=="Installment") {
                document.getElementById("monthly").disabled = true;
                document.getElementById("install").disabled = false;
            }
            else {
                document.getElementById("install").disabled = true;
            }

        }
    </script>


</head>



<body onload="myfunction1()">
    <div id="wrapper">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/Home/adminpage">Admin Panel</a>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active"><a href="~/Home/manage_fee"><i class="fa fa-bullseye"></i> Dashboard</a></li>
               
                    
                </ul>
                <ul class="nav navbar-nav navbar-right navbar-user">
                    
                     <li class="dropdown user-dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Admin<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="adminpage"><i class="fa fa-user"></i> Profile</a></li>
                          
                            <li class="divider"></li>
                            <li><a href="/Home/logout"><i class="fa fa-power-off"></i> Log Out </a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>


       <div>
        <div class="row text-center">
           
        </div>
           </div>


        <form action="~/Home/other_installment2"  name="myform" method="post">   


            <div>
            <label for="section" class="col-md-2">
              Registeration Number:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="reg" name="reg" value="@Model.reg_num" readonly>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
             <br /> <br /> <br />
             <div>
            <label for="section" class="col-md-2">
              Name:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="name" name="name" value="@Model.stud_name" readonly>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
             <br /> <br /> <br />
            
            <div>
            <label for="section" class="col-md-2">
              Degree:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="deg" name="deg" value="@Model.degree" readonly>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
             <br /> <br /> <br />
            <div>
            <label for="section" class="col-md-2">
              Fee Type:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="fee_type" name="fee_type" value="@Model.fee_type" readonly>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
              <br /> <br /> <br />
            <div>
            <label for="section" class="col-md-2">
              Amount:
            </label>
            <div class="col-md-9">
                <input type="text" class="form-control" id="amount" name="amount" value=@ViewBag.fee readonly>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
              <br /> <br /> <br />


            <div>
            <label for="section" class="col-md-2">
             Select Installment NO:
            </label>
            <div class="col-md-9">
                <select class="form-control" name="install" id="install">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                    <option value="6">6</option>
                </select>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
              <br /> <br /> <br />

             <div>
            <label for="section" class="col-md-2">
             Select Installment NO:
            </label>
            <div class="col-md-9">
                <select class="form-control" name="monthly" id="monthly">
                    <option value="1">1 Instalment ------- 1 Month</option>
                    <option value="2">1 Instalment ------- 2 Month</option>
                    <option value="3">1 Instalment ------- 3 Month</option>
                    <option value="4">1 Instalment ------- 4 Month</option>
                    <option value="5">2 Instalment ------- 1 Month</option>
                    <option value="6">2 Instalment ------- 2 Month</option>
                    <option value="7">2 Instalment ------- 3 Month</option>
                    <option value="8">2 Instalment ------- 4 Month</option>
                    <option value="9">3 Instalment ------- 1 Month</option>
                    <option value="10">3 Instalment ------- 2 Month</option>
                    <option value="11">3 Instalment ------- 3 Month</option>
                    <option value="12">3 Instalment ------- 4 Month</option>
                    <option value="13">4 Instalment ------- 1 Month</option>
                    <option value="14">4 Instalment ------- 2 Month</option>
                    <option value="15">4 Instalment ------- 3 Month</option>
                    <option value="16">4 Instalment ------- 4 Month</option>
                    <option value="17">5 Instalment ------- 1 Month</option>
                    <option value="18">5 Instalment ------- 2 Month</option>
                    <option value="19">5 Instalment ------- 3 Month</option>
                    <option value="20">5 Instalment ------- 4 Month</option>
                    <option value="21">6 Instalment ------- 1 Month</option>
                    <option value="22">6 Instalment ------- 2 Month</option>
                    <option value="23">6 Instalment ------- 3 Month</option>
                    <option value="24">6 Instalment ------- 4 Month</option>
                    
                </select>
                
                
            </div>
             <div class="col-md-1">
                <i class="fa fa-lock fa-2x"></i>
            </div>
        </div>
              <br /> <br /> <br />
              
          
        <div class="row">
            <div class="col-md-2">
            </div>
            <div class="col-md-10">
                <button type="submit" class="btn btn-info">
                    Confirm
                </button> 

        </div>
            </div>
           
             </form>   
    </div>  
    

   
  
    

</body>
</html>









